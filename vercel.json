{
    "version": 2,
    "functions": {
      "functions/server.js": {
        "runtime": "nodejs18.x"
      }
    },
    "builds": [
      {
        "src": "functions/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/functions/server.js"
      }
    ]
  } 
  